@model IEnumerable<WebMVC_BookStore.Models.ChuDe>

@{
    ViewBag.Title = "ChuDe";
    Layout = "~/Views/Layout/_LayoutAdmin.cshtml";
}

<div class="section-title-5 mb-30">
    <h2>Quản Lý Chủ Đề Sách</h2>
</div>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenChuDe)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenChuDe)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaChuDe }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaChuDe }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaChuDe })
        </td>
    </tr>
}

</table>
